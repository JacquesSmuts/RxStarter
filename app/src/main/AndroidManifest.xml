<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jacquessmuts.rxstarter">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".java.ASingleActivity" />
        <activity android:name=".java.CSimpleOperatorsActivity" />
        <activity android:name=".java.sample.ButtonRapidClickActivity"/>
        <activity android:name=".java.sample.ThreadSwitchActivity"/>
        <activity android:name=".java.sample.FailingApiCallActivity"/>
        <activity android:name=".java.sample.LongApiCallActivity"/>
        <activity android:name=".java.sample.DistinctActivity"/>
    </application>

</manifest>